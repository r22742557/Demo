<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="7" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.demo.connect"
            android:screenOrientation="portrait" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.demo.WifiScanActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.demo.PasswordActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.example.demo.Choice_main"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Automatically"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Manually"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_3"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_4"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_5"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_3_m"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_4_m"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.demo.Map_5_m"
            android:screenOrientation="portrait" />
    </application>

</manifest>